{
  "name": "open-joblist",
  "version": "1.0.0",
  "description": "Yet another React Tutorial.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "./node_modules/.bin/webpack",
    "watch:server": "./node_modules/.bin/nodemon -e ts,tsx,js --watch src index.js",
    "start": "node index.js"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateuszsokola/open-joblist.git"
  },
  "author": "msokola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateuszsokola/open-joblist/issues"
  },
  "homepage": "https://github.com/mateuszsokola/open-joblist#readme",
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "awesome-typescript-loader": "^3.2.3",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.11",
    "@types/jest": "^21.1.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "react-test-renderer": "^16.0.0"
  }
}
